<%-include("header.ejs")%>
<div  id="inventory" class="container " style="background-color: azure;" >
    <section class="jumbotron text-center">
        <div class=" jumbo-head container">
            <h1 class="jumbotron-heading">Scheduled Customers</h1>
          
           
        </div>
    </section>
        <section class="container">
            <button class="btn login_btn " id="pending"><a class="nav-link" href="/admin/search/pending" style="display:inline  ; color:black" class="btn login_btn ">Pending</a></button>

        
              
<input type="search" name="search" class="light-table-filter searchBox" data-table="order-table" placeholder="Search the consumers">
            
                <table class="order-table table table-striped">
                    <thead>
                        <tr>
                            <th>CARD NUMBER</th>
                            <th>NAME</th>
                            <th>STATUS</th>
                            
                        </tr>
                    </thead>
                    <tbody>
                        <%list.forEach((row)=>{%>
                            <tr>
                                <td><%=row.card_no%></td>
                                <td><%=row.name%></td>
                                <td><%=row.status%></td>
                            </tr>
                            
                            <%})%>
                    </tbody>
                </table>
            
            </section>

    
   </div>



<%-include("footer.ejs")%>
